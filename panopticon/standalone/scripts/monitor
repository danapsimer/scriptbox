root=$(dirname $0)/..
set -x
JAVA_OPTS="-Dlogback.configurationFile=panopticon-logback.xml"
JAVA_OPTS="$JAVA_OPTS -Djetty.port=8092"
JAVA_OPTS="$JAVA_OPTS -Xdebug -Xrunjdwp:transport=dt_socket,server=y,suspend=n,address=8787"
java $JAVA_OPTS -cp $root/target/panopticon.sa-1.0-SNAPSHOT-standalone-distro.jar org.scriptbox.panopticon.main.MonitorMain "$@"
